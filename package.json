    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-external-helpers": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/plugin-syntax-async-generators": "^7.0.0-beta.54",
    "@babel/plugin-transform-regenerator": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@simpo/rollup-vinyl-adaptor": "^1.26.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-async-functions": "^7.0.0-beta.0",
    "babel-plugin-transform-async-generator-functions": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "gulp": "^4.0.0",
    "gulp-ignore": "^2.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "^3.0.1",
    "mocha": "^5.2.0",
    "rollup": "^0.63.4",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "gulp-uglify-es": "^1.0.4"
  },
  "gulp": {
    "source": {
      "browser": "./src/browser.js",
      "server": "./src/server.js"
    },
    "dest": {
      "browser": "./umd",
      "server": "./cjs"
    },
    "nodeResolve": {
      "jsnext": true,
      "main": true,
      "extensions": [
        ".js",
        ".json"
      ],
      "browser": true
    },
    "bableGeneratorOpts": {
      "compact": true,
      "quotes": "double",
      "sourceMaps": true
    },
    "babelBrowser": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "chrome < 31",
                "firefox < 27",
                "ie 11",
                "safari 7"
              ]
            },
            "useBuiltIns": false,
            "forceAllTransforms": true
          }
        ]
      ],
      "plugins": [
        "syntax-async-functions",
        "@babel/syntax-async-generators",
        "transform-async-generator-functions",
        "@babel/transform-regenerator",
        "@babel/plugin-proposal-object-rest-spread"
      ]
    },
    "babelServer": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false,
            "targets": {
              "node": "current"
            },
            "useBuiltIns": false,
            "forceAllTransforms": true
          }
        ]
      ],
      "plugins": []
    }
  }
}
